{
  "extends": ["airbnb/base"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "max-len": ["error", { "code": 120 }],
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "always",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ]
  }
}
